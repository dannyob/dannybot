{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends" : "eslint:recommended",
  "rules": {
    "no-throw-literal": 1,
    "semi": [2, "always"],
    "eol-last": 1,
    "no-new-require": 1,
    "no-sync": 1,
    "no-console": 0,
    "no-mixed-requires": [1, false]
  }
}
