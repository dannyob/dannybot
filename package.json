{
  "name": "dannybot",
  "version": "1.0.0",
  "description": "My status dashboard, now a desktop app c/o electron.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive --require ./test/setup.js $(find test -name '*.spec.js')",
    "test-watch": "npm test -- --watch",
    "lint": "eslint src/**/*.js test/**/*.js",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannyob/dannybot.git"
  },
  "keywords": [
    "Danny",
    "bot",
    "todo",
    "statistics",
    "quantified"
  ],
  "author": "Danny O'Brien <danny@spesh.com> (http://www.spesh.com/danny/)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dannyob/dannybot/issues"
  },
  "homepage": "https://github.com/dannyob/dannybot#README.md",
  "devDependencies": {
    "chai": "^3.4.1",
    "electron-prebuilt": "^0.35.4",
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2"
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai-as-promised": "^5.2.0",
  },
  "dependencies": {
    "menubar": "^2.3.0",
    "q": "^1.4.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "sqlite3": "^3.1.1",
    "underscore": "^1.8.3"
  }
}
